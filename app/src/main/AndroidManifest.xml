<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quack">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Quack">
        <activity android:name=".UrlSite.YouTubeUrl"
            android:label="Movie Trailor"></activity>
        <activity
            android:name=".UrlSite.WebViewUrl"
            android:label="Review" />
        <activity
            android:name=".Activity.HighRatingActivity"
            android:label="@string/high_ratings"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.MovieDetailsActivity"
            android:label="Movie Details"
            android:parentActivityName=".Activity.MainActivity" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>